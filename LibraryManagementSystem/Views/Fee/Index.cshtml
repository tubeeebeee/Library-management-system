
@{
    ViewBag.Title = "Index";
}
<div class="container" style="margin-top:2%;">
    @{
        int rowNo = 0;
    }
    @if (ViewBag.feeList != null)
    {
        <table class="table table-striped table-responsive">
            <thead>
                <tr>
                    <th>STT</th>
                    <th>Name</th>
                    <th>fee</th>
                    <th>Note</th>
                    <th></th>
                </tr>
            </thead>
            <tbody id="getRow">


                @foreach (var item in ViewBag.feeList)
                {

                    <tr>

                        <td>@(rowNo++)</td>
                        <td>@item.name</td>
                        <td>@item.fee</td>
                        <td>@item.note</td>
                    </tr>
                }

            </tbody>
        </table>
    }
    
</div>

